<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".hamburger">

    <TextView
        android:id="@+id/mainviewhamburger"
        android:layout_width="414dp"
        android:layout_height="178dp"
        android:layout_weight="0.05"
        android:background="#FFE34020"
        android:gravity="center|center_horizontal|center_vertical"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/btn_setting_notice2"
        android:layout_width="420dp"
        android:layout_height="56dp"
        android:layout_gravity="center"
        android:text="공지사항"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="#FFE34020"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_setting_quit2" />

    <Button
        android:id="@+id/btn_setting_change2"
        android:layout_width="415dp"
        android:layout_height="57dp"
        android:layout_gravity="center"
        android:text="어플소개"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="#FFE34020"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_setting_notice2" />

    <Button
        android:id="@+id/btn_setting_quit2"
        android:layout_width="417dp"
        android:layout_height="55dp"
        android:layout_gravity="center"
        android:layout_marginTop="100dp"
        android:text="마이페이지"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="#FFE34020"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mainviewhamburger" />

    <Button
        android:id="@+id/btn_setting_qa2"
        android:layout_width="427dp"
        android:layout_height="53dp"
        android:layout_gravity="center"
        android:text="Q&amp;A"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="#FFE34020"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_setting_change2" />

    <Button
        android:id="@+id/btn_setting_as2"
        android:layout_width="421dp"
        android:layout_height="56dp"
        android:layout_gravity="center"
        android:text="고객센터"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="#FFE34020"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_setting_qa2" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="321dp"
        android:layout_height="35dp"
        android:layout_marginTop="64dp"
        android:text="로그인 해주세요"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.377"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_hamburgerX"
        android:layout_width="51dp"
        android:layout_height="37dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_close_24"
        tools:ignore="VectorDrawableCompat" />

    <Button
        android:id="@+id/btn_loginhamburger"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:text="로그인"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="#E34020"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <Button
        android:id="@+id/btn_joinhamburger"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="회원가입"
        android:textSize="16sp"
        android:textStyle="bold"
        app:backgroundTint="#E34020"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btn_loginhamburger"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

</androidx.constraintlayout.widget.ConstraintLayout>